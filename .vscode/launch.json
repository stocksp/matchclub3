{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "port": 9229
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Chrome on localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/pathto/myapp",
      "sourceMapPathOverrides": {
          "webpack://_N_E/*": "${webRoot}/*"
      }
  }
  ]
}